<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.dtstack.rdos.engine.entrance.db.mapper.RdosServerLogMapper">
    <resultMap id="rdosServerLog" type="com.dtstack.rdos.engine.entrance.db.dataobject.RdosServerLog">
        <id column="id" property="id" jdbcType="BIGINT" />
        <result column="task_id" property="taskId" jdbcType="VARCHAR" />
        <result column="action_log_id" property="actionLogId" jdbcType="BIGINT" />
        <result column="log_info" property="logInfo" jdbcType="VARCHAR" />
        <result column="gmt_create" property="gmtCreate" jdbcType="TIMESTAMP" />
        <result column="gmt_modified" property="gmtModified" jdbcType="TIMESTAMP" />
        <result column="is_deleted" property="isDeleted" jdbcType="BIT"/>
    </resultMap>

    <insert id="insertSvrLog" parameterType="java.util.HashMap">
        insert into rdos_server_log(task_id, action_log_id,log_info, gmt_create, gmt_modified)
        values(#{taskId}, #{actionLogId},#{logInfo}, now(), now());
    </insert>
</mapper>